{
   "manifest_version":2,
   "name":"Droner to Döner",
   "description":"Udskifter \"Droner\" med \"Döner\"",
   "version":"1.1",
   "icons":{
      "16":"icon16.png",
      "48":"icon48.png",
      "128":"icon128.png"
   },
    "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["contentscript.js"],
	    "run_at": "document_idle"
    }
  ],
   "permissions":[
     "http://*/*"
   ]
}
